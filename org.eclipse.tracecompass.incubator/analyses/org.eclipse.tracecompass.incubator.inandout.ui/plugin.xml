<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.eclipse.tracecompass.incubator.inandout.ui.configure"
               icon="icons/prd_config_wiz.gif"
               label="%command.label"
               style="push"
               tooltip="%command.tooltip">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.tracecompass.incubator.inandout.ui.isInAndOut">
                     </test>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.linuxtools.tmf.ui.commands.category"
            description="%command.description"
            id="org.eclipse.tracecompass.incubator.inandout.ui.configure"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.tracecompass.incubator.internal.inandout.ui.InAndOutHandler"
            commandId="org.eclipse.tracecompass.incubator.inandout.ui.configure">
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.eclipse.tracecompass.incubator.internal.inandout.ui.InAndOutEnabler"
            id="org.eclipse.tracecompass.incubator.inandout.ui.enabler"
            namespace="org.eclipse.tracecompass.incubator.inandout.ui"
            properties="isInAndOut"
            type="org.eclipse.tracecompass.tmf.ui.project.model.TmfAnalysisElement">
      </propertyTester>
   </extension>
</plugin>
